to-report like-tags-in-topk [topk]
  let like-list []
  
  foreach topk [
    let curr-turtle ?
    foreach [tags] of self [
      show self
      if (member? ? [tags] of curr-turtle) [
        set like-list lput curr-turtle like-list
      ]
    ] 
  ]
  
  draw-connections like-list
  report like-list
end

